<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.singsound.singsounddemo">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.base.BaseCloudActivity" />
        <activity android:name=".activity.Word_OnlineCloudActivity" />
        <activity android:name=".activity.AboutActivity" />
        <activity android:name=".activity.base.BaseNativeActivity" />
        <activity android:name=".activity.Sentence_OnlineCloudActivity" />
        <activity android:name=".activity.Word_NativeActivity" />
        <activity android:name=".activity.Sentence_NativeActivity" />
        <activity android:name=".activity.Choice_OnlineCloudActivity" />
        <activity android:name=".activity.Article_OnlineCloudActivity" />
        <activity android:name=".activity.QuestionAnswer_OnlineCloudActivity"></activity>
    </application>

</manifest>